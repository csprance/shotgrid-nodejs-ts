{
  "name": "shotgrid-nodejs-ts",
  "version": "5.0.0",
  "author": "csprance",
  "keywords": [
    "shotgrid",
    "shotgun"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint src test --fix",
    "prepare": "tsdx build --target node"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrder": [
      "^@/(.*)$",
      "^[./]"
    ]
  },
  "module": "dist/shotgrid-nodejs-ts.esm.js",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/async-retry": "^1.4.4",
    "@types/node-fetch": "^2.6.2",
    "@types/qs": "^6.9.7",
    "@types/require-all": "^3.0.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "eslint-plugin-prettier": "latest",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "file-type": "^15.0.1",
    "node-fetch": "^2.6.7",
    "qs": "^6.10.5",
    "require-all": "^3.0.0"
  },
  "resolutions": {
    "**/tsdx/jest": "27.5.1",
    "**/tsdx/ts-jest": "27.1.5",
    "**/tsdx/eslint": "^7.32.0"
  }
}
